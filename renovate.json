{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:base"
  ],
  "enabledManagers": ["npm", "poetry", "dockerfile", "docker-compose", "maven", "maven-wrapper", "cargo", "gomod", "bundler", "helmv3"],
  "packageRules": [
    {
      "matchDatasources": ["docker"],
      "matchDepNames": ["otel-catalog", "otel-pricing", "otel-recommendations", "otel-inventory",
                        "otel-warehouse-us", "otel-warehouse-eu", "otel-warehouse-jp", "otel-warehouse-uk", "otel-analytics"],
      "enabled": false
    }
  ]

}
